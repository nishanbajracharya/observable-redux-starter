{
  "name": "react-redux-observable-starter",
  "version": "1.0.0",
  "description": "A starter kit for developing single page applications using react, redux, recompose and redux-observable.",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "webpack-dev-server --config webpack/webpack.config.js",
    "build": "rimraf dist && webpack --config webpack/webpack.config.js --progress --colors",
    "build:production": "rimraf dist && NODE_ENV=production webpack --config ./webpack/webpack.config.production.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nishanbajracharya/react-redux-observable-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "recompose",
    "redux-observable"
  ],
  "author": "Nishan Bajracharya <nisbaj11@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nishanbajracharya/react-redux-observable-starter/issues"
  },
  "homepage": "https://github.com/nishanbajracharya/react-redux-observable-starter#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.0.0",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
